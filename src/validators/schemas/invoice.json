{
  "$id": "http://example.com/example.json",
  "type": "object",
  "minProperties": 4,
  "maxProperties": 4,
  "additionalProperties": false,
  "properties": {
    "orderId": {
      "$id": "/properties/orderId",
      "type": "string",
      "title": "The Orderid Schema ",
      "minLength": 64,
      "maxLength": 64
    },
    "manifest": {
      "$id": "/properties/manifest",
      "type": "array",
      "minItems": 1,
      "items": {
        "$id": "/properties/manifest/items",
        "type": "object",
        "minProperties": 4,
        "maxProperties": 4,
        "additionalProperties": false,
        "properties": {
          "item": {
            "$id": "/properties/manifest/items/properties/item",
            "type": "string",
            "title": "The Item Schema ",
            "examples": [
              "chicken korma"
            ],
            "maxLength": 255
          },
          "quantity": {
            "$id": "/properties/manifest/items/properties/quantity",
            "type": "integer",
            "title": "The Quantity Schema ",
            "examples": [
              2
            ]
          },
          "price": {
            "$id": "/properties/manifest/items/properties/price",
            "type": "number",
            "title": "The Price Schema ",
            "examples": [
              20
            ]
          },
          "tax": {
            "$id": "/properties/manifest/items/properties/tax",
            "type": "number",
            "title": "The Tax Schema ",
            "examples": [
              0.05000000074505806
            ],
            "minimum": 0,
            "maximum": 1
          }
        },
        "required": [
          "item",
          "quantity",
          "price",
          "tax"
        ]
      }
    },
    "totalPreTax": {
      "$id": "/properties/totalPreTax",
      "type": "number",
      "title": "The Totalpretax Schema ",
      "examples": [
        64
      ]
    },
    "totalPostTax": {
      "$id": "/properties/totalPostTax",
      "type": "number",
      "title": "The Totalposttax Schema ",
      "examples": [
        67.19999694824219
      ]
    }
  },
  "required": [
    "orderId",
    "manifest",
    "totalPreTax",
    "totalPostTax"
  ]
}
